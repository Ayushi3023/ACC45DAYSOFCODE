#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,b=0;
	    cin>>n;
	    int a[n];
	    
	    for(int i=0; i<n; i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        if(a[i]%2)
	        b++;
	    }
	    if(n==1)
	    cout<<"NO"<<endl;
	    else if(b==0 || b%2)
	    cout<<"NO"<<endl;
	    else
	    cout<<"YES"<<endl;
	}
	return 0;
}
